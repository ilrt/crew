  <rdf:Description rdf:about="http://www.crew-vre.net/events/swig-uk-2008-11-11/talks/#chris-dollin-one-rules">
    <crew:hasRecordingId>1226657429920448936606</crew:hasRecordingId>
    <eswc:hasRole rdf:resource="http://www.crew-vre.net/events/swig-uk-2008-11-11/role/#presenter-chris-dollin"/>
