  <rdf:Description rdf:about="http://www.crew-vre.net/events/swig-uk-2008-11-11/talks/#alistair-miles-flyweb-project">
    <crew:hasRecordingId>1226655690218586630600</crew:hasRecordingId>
    <eswc:hasRole rdf:resource="http://www.crew-vre.net/events/swig-uk-2008-11-11/role/#presenter-graham-klyne"/>
