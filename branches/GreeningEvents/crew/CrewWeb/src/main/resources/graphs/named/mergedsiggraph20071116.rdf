  <rdf:Description rdf:about="http://www.crew-vre.net/events#sigraph-2007-11-16">
    <eswc:hasLocation rdf:resource="http://www.crew-vrew.net/places/manchester/kilburn/1.10/"/>
    <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Interesting? Aesthetic? Useful? GeoVisualization Perspectives and Examples</dc:title>
    <dc:creation-date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2008-02-19T15:37:25+00:00</dc:creation-date>
    <iugo:hasLocation rdf:resource="http://www.ilrt.bristol.ac.uk/iugo/location/#manchester"/>
