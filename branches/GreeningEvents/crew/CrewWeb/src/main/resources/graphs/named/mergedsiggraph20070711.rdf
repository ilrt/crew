    <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Molecular Magnetism: Questions, Tools, and Answers</dc:title>
    <rdf:type rdf:resource="http://www.eswc2006.org/technologies/ontology#PaperPresentation"/>
