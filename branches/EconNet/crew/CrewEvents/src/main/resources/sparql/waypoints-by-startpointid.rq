PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX ge: <http://www.ilrt.bris.ac.uk/GreeningEvents/ontology#>
SELECT $graph $id $waypoint $latitude $longitude
WHERE {
    GRAPH $graph {
        $id ge:waypoint $waypoint .
        $waypoint pos:lat $latitude .
        $waypoint pos:long $longitude .
    }
}